	spi_bb_rom: spi-bb-rom {
		pinmux {
			function = "gpio";
			pins = "gpio90", "gpio91", "gpio92", "gpio95";
		};
		cs {
			pins = "gpio90";
			bias-disable;
			output-high;
		};
		clk {
			pins = "gpio95";
			bias-pull-down;
			output-low;
		};
