&tlmm {
	ap_rst_req: ap-rst-req {
		pinmux {
			pins = "gpio116";
			function = "gpio";
		};

		pinconf {
			pins = "gpio116";
			bias-disable;
		};
	};

	ap_edp_bklten: ap-edp-bklten {
		pinmux {
			pins = "gpio37";
			function = "gpio";
		};

		pinconf {
			pins = "gpio37";
			drive-strength = <8>; /* TODO: why? */
			bias-disable;
		};
	};

	edp_brij_en: edp-brij-en {
		pinmux {
			pins = "gpio102";
			function = "gpio";
		};

		pinconf {
			pins = "gpio102";
			drive-strength = <8>; /* TODO: why? */
			bias-disable;
		};
	};

	en_pp3300_dx_edp: en-pp3300-dx-edp {
